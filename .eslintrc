{
    "ecmaFeatures": {
        "modules": true
    },
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {
        "it": false
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "comma-dangle": 0,
        "indent": [2, 4, {"SwitchCase": 1}],
    }
}
